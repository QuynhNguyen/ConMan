%h1 User

%table#users
  %thead
    %tr
      %th Username
      %th First Name
      %th Last Name
      %th Email
      %th Admin
      %th Edit
    %tbody
      - @users.each do |user|
        %tr
          %td= user.username
          %td= user.first_name
          %td= user.last_name
          %td= user.email
          %td= user.admin
          %td= link_to "Edit", edit_user_path(user)
         

= link_to 'Add new user', new_user_path

