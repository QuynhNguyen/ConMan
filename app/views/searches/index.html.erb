<h3>Search Result</h3>
<%if @result.empty? %>
"No Matched"
<% else %>
<table class="table table-hover">
	<tr>
		<th>Username </th>
		<th>First Name</th>
		<th>Last Name</th>
	</tr>
<% @result.each do |t| %>
	<tr>
		<td><a href=<%= "profiles/#{t.id}" %>><%= t.username %></a> </td>
		<td><%= t.first_name %></td>
		<td><%= t.last_name %></td>
	</tr>
<% end %>
</table>
<% end %>
