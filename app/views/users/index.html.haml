%h1 User

%table#users
  %thead
    %tr
      %tr Username
      %tr First Name
      %tr Last Name
      %tr Email
      %tr Admin
      %tr More info
    %tbody
      - @users.each do |user|
        %td= user.nickname
        %td= user.first_name
        %td= user.last_name
        %td= user.email
        %td= user.admin
        %td= link_to "More about #{user.first_name}", user_path(user) 

= link_to 'Add new user', new_user_path

