%h1 Google#index
%p Find me in app/views/google/index.html.haml

= @gmail.inbox.emails(:unread).each do |m|
	= m.to_s

- if(@titles)
	= link_to 'Add', controller: :google, action: :insert_contact, access_token: @token
	%h3 Gmail contact list
	- @titles.each_with_index do |title,index|
		%p
			%b= title
			= link_to "#{@emails[index]}", "#{@emails[index]}"
			= link_to 'Delete', controller: :google, action: :delete_contact, access_token: @token, id: "#{@id[index]}"
			= link_to 'Direct Delete', "https://www.google.com/m8/feeds/contacts/default/full/#{@id[index]}?access_token=#{@token}", method: :delete

=@response